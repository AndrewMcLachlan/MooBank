<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="GitHub" value="https://nuget.pkg.github.com/AndrewMclachlan/index.json" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
    <packageSource key="GitHub">
      <package pattern="ASM.*" />
      <package pattern="ASM" />
    </packageSource>
  </packageSourceMapping>
</configuration>
