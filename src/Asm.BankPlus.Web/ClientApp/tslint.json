{
    "extends": [ "tslint:recommended", "tslint-react" ],
    "rules": {
      "max-line-length": {
        "options": [ 512 ]
      },
      "no-bitwise": false,
      "interface-name": [ true, "never-prefix" ],
      "curly": [ true, "ignore-same-line" ],
      "one-line": false,
      "variable-name": [ true, "allow-leading-underscore", "allow-pascal-case" ],
      "no-console": false,
      "object-literal-sort-keys": false
    }
  }
  